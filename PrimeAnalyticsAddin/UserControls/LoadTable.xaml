<Window x:Class="PrimeAnalyticsAddin.UserControls.LoadTable"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrimeAnalyticsAddin.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800"
        Title="Load Data" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closing="Window_Closing"
        >
    <Grid>
        <DataGrid x:Name="dataGrid" Margin="0,48,0,0" >


        </DataGrid>
        <Label x:Name="label" Content="Select Process" HorizontalAlignment="Left" VerticalAlignment="Top" Width="112" Margin="10,8,0,0"/>
        <ComboBox x:Name="cboxProcess" HorizontalAlignment="Left" Margin="122,10,0,0" VerticalAlignment="Top" Width="302" Initialized="cboxProcess_Initialized"/>
        <Button x:Name="btnGet" Content="Display" HorizontalAlignment="Left" Margin="437,12,0,0" VerticalAlignment="Top" Width="74" Click="btnGet_Click"/>
        <Button x:Name="btnImport" Content="Import" Margin="0,12,9.6,0" Height="19" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="btnImport_Click"/>

    </Grid>
</Window>
